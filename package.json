		"es6",
		"es2015",
		"yield",
		"function",
		"function*"
	],
	"author": "Jordan Harband",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ljharb/is-generator-function/issues"
	},
	"dependencies": {},
	"devDependencies": {
		"@ljharb/eslint-config": "^12.2.1",
		"core-js": "^2.5.6",
		"covert": "^1.1.0",
		"eslint": "^4.19.1",
		"make-generator-function": "^1.1.0",
		"nsp": "^3.2.1",
		"replace": "^1.0.0",
		"semver": "^5.5.0",
		"tape": "^4.9.0",
		"uglify-register": "^1.0.1"
	},
	"testling": {
		"files": "test/index.js",
